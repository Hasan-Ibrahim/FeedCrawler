<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Feed Crawler For Donald Trump</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>

    <div class="container body-content" data-ng-app="feedCrawlerModule">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Feed Crawler</p>
        </footer>
    </div>
    <script src="~/Scripts/angular.js"></script>
    <script src="~/Scripts/ui-bootstrap-tpls.js"></script>

    <script src="~/Scripts/app/feedCrawlerModule.js"></script>
    <script src="~/Scripts/app/services/cnnFeedService.js"></script>
    <script src="~/Scripts/app/services/twitterFeedService.js"></script>
    <script src="~/Scripts/app/controllers/cnnFeedController.js"></script>
    <script src="~/Scripts/app/controllers/twitterFeedController.js"></script>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
